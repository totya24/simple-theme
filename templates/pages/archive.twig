{% extends "master.twig" %}
{% block content %}
<div class="uk-container">
    {% for post in posts %}
    <div class="uk-card uk-card-default uk-card-body uk-margin-top">
        <h3 class="uk-card-title">{{ post.title }}</h3>
        {{ post.content }}
    </div>
    {% endfor %}
</div>
{% endblock %}